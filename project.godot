[application]

config/name="Voxiverse"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.0", "Vulkan Clustered")
config/icon="res://assets/textures/block/grass_block_side.png"

[audio]

buses/default_bus_layout="res://data/audio/bus_layout.tres"

[autoload]

Signals="*res://autoload/signals.gd"
